{
  "compilerOptions": {
    "esModuleInterop": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "lib": ["esnext", "dom"],
    "target": "es5",
    "strict": true
  }
}
